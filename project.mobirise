    "page27.html": {
      "settings": {
        "meta_descr": "Website Maker Description",
        "title": "PURVA BADHRAPADA",
        "order": 16
      },
      "components": [
        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
